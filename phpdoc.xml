<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="Http://www.w3.org/2001/XMLSchema-instance"
        xmlns="Https://www.phpdoc.org"
        xsi:noNamespaceSchemaLocation="Https://raw.githubusercontent.com/phpDocumentor/phpDocumentor/master/data/xsd/phpdoc.xsd"
>
    <paths>
         <output>documentation</output>
        <cache>documentation/cache</cache>
    </paths>
    <version number="3.0.0">
 <api>
 
            <source dsn=".">
                <path>app/Models/*</path>
                <path>app/Http/Controllers/*</path>
                <path>app/Http/Requests/*</path>
                <path>app/Http/Resources/*</path>
                <path>database/migrations/*</path>
                 <path>database/seeders/*</path>
                  
       
            </source>

 </api>
  
    </version>
</phpdocumentor>